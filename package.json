{
  "name": "udemy-nodejs-angular2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts' 'assets/app/**/*.ngsummary.json' 'assets/app/**/*.ngstyle.ts'"
  },
  "dependencies": {
    "@angular/animations": "^10.0.5",
    "@angular/common": "^10.0.5",
    "@angular/compiler": "^10.0.5",
    "@angular/compiler-cli": "^10.0.5",
    "@angular/core": "^10.0.5",
    "@angular/forms": "^10.0.5",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^10.0.5",
    "@angular/platform-browser-dynamic": "^10.0.5",
    "@angular/platform-server": "^10.0.5",
    "@angular/router": "^10.0.5",
    "@angular/upgrade": "^10.0.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.19.0",
    "cookie-parser": "~1.4.5",
    "core-js": "^3.6.5",
    "debug": "~4.1.1",
    "express": "~4.17.3",
    "hbs": "~4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "serve-favicon": "~2.5.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@types/core-js": "2.5.3",
    "@types/node": "^14.0.24",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "del-cli": "^3.0.1",
    "html-loader": "^1.1.0",
    "raw-loader": "^4.0.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-merge": "^5.0.9"
  }
}
